<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AspectImageView">
        <attr name="aspectRatio" format="float" />
        <attr name="aspectRatioEnabled" format="boolean" />
        <attr name="dominantMeasurement">
            <enum name="width" value="0"/>
            <enum name="height" value="1"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="BlurImageArtist">
        <attr name="imageSrc" format="reference"/>
        <attr name="imagePlaceholder" format="reference"/>
        <attr name="imageError" format="reference"/>
        <attr name="blur" format="reference"/>
    </declare-styleable>

</resources>